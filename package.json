{
  "name": "research-report-generator",
  "version": "1.0.0",
  "description": "Autonomous research report generator with multi-source data synthesis",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "serve": "tsx src/server/app.ts",
    "cli": "tsx src/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "research",
    "report",
    "generator",
    "ai",
    "scraping",
    "synthesis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/static": "^8.2.0",
    "@octokit/rest": "^22.0.0",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "commander": "^14.0.0",
    "dotenv": "^17.2.2",
    "fastify": "^5.5.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "pino-pretty": "^13.1.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
